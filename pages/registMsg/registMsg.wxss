/* pages/registMsg/registMsg.wxss */
page{
  padding: 12rpx 30rpx 0;
}
.item{
  margin-top: 33rpx;
  border-bottom: 1px solid #f2f2f2;
  height: 68rpx;
  line-height: 68rpx;
  font-size: 28rpx;
  font-weight: bold;
}
.item .title{
  display: inline-block;
  width: 220rpx;
}
.item input,.item picker{
  display: inline-block;
  width: 490rpx;
  height: 68rpx;
  line-height: 68rpx;
  vertical-align: middle;
}
.button button{
  margin: 80rpx auto;
  width: 325rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 20rpx;
  color: #fff;
  font-size: 28rpx;
  font-weight: bold;
  background: #e4373b;
}