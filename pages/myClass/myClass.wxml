<!--pages/myClass/myClass.wxml-->
<form bindsubmit="formSubmit">
  <view class='item'>
    <text class='title'>毕业时间</text>
    <picker mode="date" bindchange="bindgraduationTimeChange" value="{{graduationTime}}" fields="year" name="graduationTime">
      <view class="picker">{{graduationTime}}</view>
    </picker>
  </view>
  <view class='item'>
    <text class='title'>考入大学</text>
    <input name="university" bindchange="binduniversityChange" value="{{university}}" placeholder='请输入学校名称'></input>
  </view>
  <button form-type="submit">修改</button>
</form>